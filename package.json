{
  "name": "website",
  "version": "1.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build && next-export-optimize-images",
    "build:analyze": "ANALYZE=true pnpm run build",
    "start": "next build && next-export-optimize-images && npx serve@latest out",
    "git-hooks-install": "husky install",
    "storybook-start": "storybook dev -p 6006",
    "storybook-build": "storybook build",
    "generate-ts-doc": "api-extractor run --local --verbose",
    "format": "prettier . --write",
    "lint:next": "next lint",
    "lint:tsc": "tsc --newLine LF",
    "lint:md": "markdownlint -i CHANGELOG.md **/*.md",
    "test:e2e": "pnpm exec playwright test ./src/test/e2e",
    "test:mutation": "npx stryker run",
    "test:visual": "pnpm exec playwright test ./src/test/visual",
    "test:unit": "jest --verbose",
    "test:memory-leak": "node ./src/test/memory-leak/runMemlabTests",
    "lighthouse:desktop": "next build && next-export-optimize-images && lhci autorun --config=lighthouserc.desktop.js --collect.startServerCommand=\"npx serve@latest out\"",
    "lighthouse:mobile": "next build && next-export-optimize-images && lhci autorun --config=lighthouserc.mobile.js --collect.startServerCommand=\"npx serve@latest out\"",
    "lighthouse:desktop-autorun": "lhci autorun --config=lighthouserc.desktop.js",
    "lighthouse:mobile-autorun": "lhci autorun --config=lighthouserc.mobile.js",
    "check-node-version": "node checkNodeVersion.js",
    "preinstall": "pnpm run check-node-version"
  },
  "dependencies": {
    "@apollo/client": "^3.11.9",
    "@emotion/styled": "^11.13.0",
    "@mui/material": "^6.1.7",
    "@mui/system": "^6.1.7",
    "@next/bundle-analyzer": "^15.0.2",
    "@next/env": "^14.2.14",
    "dotenv": "^16.4.5",
    "graphql": "^16.9.0",
    "i18next": "^23.16.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.53.2",
    "react-i18next": "^15.0.2",
    "terser-webpack-plugin": "^5.3.10"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.6.0",
    "@commitlint/config-conventional": "^19.6.0",
    "@faker-js/faker": "^9.0.3",
    "@lhci/cli": "^0.14.0",
    "@memlab/api": "^1.0.37",
    "@memlab/heap-analysis": "^1.0.35",
    "@microsoft/api-extractor": "^7.47.9",
    "@next/eslint-plugin-next": "^15.0.2",
    "@next/third-parties": "^15.0.3",
    "@playwright/test": "^1.49.1",
    "@sentry/node": "^8.34.0",
    "@sentry/react": "^8.37.1",
    "@storybook/addon-actions": "^8.3.5",
    "@storybook/addon-essentials": "^8.3.5",
    "@storybook/addon-interactions": "^8.4.2",
    "@storybook/addon-links": "^8.3.5",
    "@storybook/addon-onboarding": "^8.3.5",
    "@storybook/blocks": "^8.3.5",
    "@storybook/nextjs": "^8.4.2",
    "@storybook/react": "^8.3.5",
    "@storybook/test": "^8.3.5",
    "@storybook/testing-library": "0.2.2",
    "@stryker-mutator/core": "^8.6.0",
    "@stryker-mutator/jest-runner": "^8.6.0",
    "@stryker-mutator/typescript": "^4.0.0",
    "@testing-library/jest-dom": "^6.6.2",
    "@testing-library/react": "^14.3.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^29.5.13",
    "@types/material-ui": "^0.21.17",
    "@types/node": "^22.9.3",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.57.1",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-next": "^15.0.3",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jest-dom": "^5.4.0",
    "eslint-plugin-jsx-a11y": "^6.10.0",
    "eslint-plugin-react": "^7.37.1",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-storybook": "^0.10.2",
    "eslint-plugin-testing-library": "^6.4.0",
    "husky": "^9.1.6",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jsdoc": "^4.0.3",
    "markdownlint-cli": "^0.39.0",
    "memlab": "^1.1.55",
    "next": "^15.0.3",
    "next-export-optimize-images": "^4.5.1",
    "prettier": "^3.3.3",
    "semver": "^7.6.3",
    "storybook": "^8.4.2",
    "swiper": "^11.1.14",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.2",
    "web-vitals": "^4.2.4",
    "webpack": "^5.95.0"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "engineStrict": true
}
