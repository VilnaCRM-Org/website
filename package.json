{
  "name": "website",
  "version": "1.3.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build && next-export-optimize-images",
    "build:analyze": "ANALYZE=true pnpm run build",
    "start-prod": "next build && next-export-optimize-images && npx serve -s out -p 3001",
    "git-hooks-install": "husky install",
    "storybook-start": "storybook dev -p 6006",
    "storybook-build": "storybook build",
    "generate-ts-doc": "api-extractor run --local --verbose",
    "format": "prettier . --write",
    "lint:next": "next lint",
    "lint:tsc": "tsc --newLine LF",
    "lint:md": "markdownlint -i CHANGELOG.md **/*.md",
    "test:e2e": "pnpm exec playwright test ./src/test/e2e",
    "test:mutation": "npx stryker run",
    "test:visual:update": "npx playwright test ./src/test/visual --update-snapshots",
    "test:visual": "pnpm exec playwright test ./src/test/visual",
    "test:unit": "TEST_ENV=client jest --verbose",
    "test:unit:all": "TEST_ENV=client jest --verbose && TEST_ENV=server jest ./src/test/apollo-server",
    "test:memory-leak": "node ./src/test/memory-leak/runMemlabTests",
    "lighthouse:desktop": "next build && next-export-optimize-images && lhci autorun --config=lighthouserc.desktop.js --collect.startServerCommand=\"npx serve out\"",
    "lighthouse:mobile": "next build && next-export-optimize-images && lhci autorun --config=lighthouserc.mobile.js --collect.startServerCommand=\"npx serve out\"",
    "lighthouse:desktop-autorun": "lhci autorun --config=lighthouserc.desktop.js",
    "lighthouse:mobile-autorun": "lhci autorun --config=lighthouserc.mobile.js",
    "check-node-version": "node checkNodeVersion.js",
    "preinstall": "pnpm run check-node-version",
    "compile-server": "tsc --project tsconfig.server.json",
    "start:apollo-server": "node ./out/docker/apollo-server/server.mjs"
  },
  "dependencies": {
    "@apollo/client": "^3.13.5",
    "@emotion/styled": "^11.14.0",
    "@mui/material": "^6.4.2",
    "@mui/system": "^6.4.2",
    "@next/bundle-analyzer": "^15.1.6",
    "@next/env": "^15.1.6",
    "dotenv": "^16.4.7",
    "graphql": "^16.10.0",
    "i18next": "^23.16.8",
    "puppeteer-core": "^24.4.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.54.2",
    "react-i18next": "^15.4.0",
    "terser-webpack-plugin": "^5.3.11",
    "wait-on": "^8.0.2",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@apollo/server": "^4.11.3",
    "@commitlint/cli": "^19.7.1",
    "@commitlint/config-conventional": "^19.7.1",
    "@faker-js/faker": "^9.4.0",
    "@lhci/cli": "^0.14.0",
    "@memlab/api": "^1.0.38",
    "@memlab/heap-analysis": "^1.0.36",
    "@microsoft/api-extractor": "^7.49.2",
    "@next/eslint-plugin-next": "^15.1.6",
    "@next/third-parties": "^15.1.6",
    "@playwright/test": "^1.51.0",
    "@sentry/node": "^8.54.0",
    "@sentry/react": "^8.54.0",
    "@storybook/addon-actions": "^8.5.3",
    "@storybook/addon-essentials": "^8.5.3",
    "@storybook/addon-interactions": "^8.5.3",
    "@storybook/addon-links": "^8.5.3",
    "@storybook/addon-onboarding": "^8.5.3",
    "@storybook/blocks": "^8.5.3",
    "@storybook/nextjs": "^8.5.3",
    "@storybook/react": "^8.5.3",
    "@storybook/test": "^8.5.3",
    "@storybook/testing-library": "0.2.2",
    "@stryker-mutator/core": "^8.7.1",
    "@stryker-mutator/jest-runner": "^8.7.1",
    "@stryker-mutator/typescript": "^4.0.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^14.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^29.5.14",
    "@types/material-ui": "^0.21.18",
    "@types/node": "^22.14.1",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.57.1",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-next": "^15.1.6",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jest-dom": "^5.5.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-storybook": "^0.10.2",
    "eslint-plugin-testing-library": "^6.5.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jsdoc": "^4.0.4",
    "markdownlint-cli": "^0.39.0",
    "memlab": "^1.1.56",
    "next": "^15.2.4",
    "next-export-optimize-images": "^4.6.1",
    "playwright": "^1.51.0",
    "prettier": "^3.4.2",
    "semver": "^7.7.1",
    "serve": "^14.2.4",
    "storybook": "^8.5.3",
    "swiper": "^11.2.2",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3",
    "web-vitals": "^4.2.4",
    "webpack": "^5.97.1"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "engineStrict": true
}
